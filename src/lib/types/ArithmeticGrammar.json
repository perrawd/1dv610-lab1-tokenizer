{
  "grammarName": "ArithmeticGrammar",
  "description": "Grammar type for Arithmetic.",
  "trim": true,
  "delimiter": "[-+*/()]|\\S(?![0-9]*[.]*[0-9])",
  "tokenTypes": {
    "NUMBER": "^[0-9]+(\\.([0-9])+)?",
    "ADD": "^[+]",
    "MUL": "^[*]",
    "SUB": "^[-]",
    "DIV": "^[*/]",
    "OPENPAREN": "^[*(]",
    "CLOSEPAREN": "^[*)]"
  }
}
